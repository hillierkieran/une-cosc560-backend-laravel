# PHPUnit cache file
/.phpunit.cache

# Node.js modules directory, generated by npm or yarn
/node_modules

# Build directory for public assets, typically generated by webpack, mix, or similar tools
/public/build

# Hot module replacement file for Laravel Mix
/public/hot

# Symbolic link for storage in Laravel (created by `php artisan storage:link`)
/public/storage

# Secret key files stored in the storage directory
/storage/*.key

# Composer dependencies directory
/vendor

# Environment configuration files for local development
.env

# Backup of the .env file
.env.backup

# Production environment configuration file
.env.production

# Phpactor configuration file (PHP refactoring tool)
/.phpactor.json

# PHPUnit result cache file
/.phpunit.result.cache

# Homestead configuration files (for Laravel's Vagrant box)
Homestead.json
Homestead.yaml

# Composer authentication file
auth.json

# npm debug log, generated on npm errors
npm-debug.log

# Yarn error log, generated on yarn errors
yarn-error.log

# JetBrains Fleet IDE configuration directory
/.fleet

# PHPStorm, WebStorm, or IntelliJ IDEA configuration directory
/.idea

# Visual Studio Code configuration directory
/.vscode

# Local History for Visual Studio Code extension (tracks file changes)
/.history

# Bash scripts
*.sh
